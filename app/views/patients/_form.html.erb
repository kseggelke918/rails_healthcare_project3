<%= form_for :patient, as: :post, url: user_patients_path(current_user) do |f| %>
    <%= f.label "Patient Name:"%>
    <%= f.text_field :name %><br>
    <%= f.label "Patient Symptoms:"%>
    <%= f.text_area :symptoms%><br>
    <%= f.label "Appointment"%>
    <%= f.datetime_select :time%>
    <br>
    <%= f.submit "Submit"%>
<% end %>